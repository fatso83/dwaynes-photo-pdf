{
  "name": "pdf-lib-browser-test",
  "version": "1.0.0",
  "description": "",
  "main": "browser-init.js",
  "scripts": {
    "build": "browserify . -d -o dist/index.js",
    "deploy": "now",
    "start": "npm run build && http-server .",
    "postinstall": "cp node_modules/pdf-lib/dist/pdf-lib.min.js assets/"
  },
  "browserify": {
    "transform": [
      [
        "browserify-shim",
        "babelify",
        {
          "sourceMap": true
        }
      ]
    ]
  },
  "browserify-shim": {
    "pdf-lib": "global:PDFLib"
  },
  "babel": {
    "presets": [
      "@babel/env"
    ],
    "plugins": []
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "pdf-lib": "0.6.1",
    "promisify": "0.0.3"
  },
  "devDependencies": {
    "@babel/cli": "7",
    "@babel/core": "7",
    "@babel/preset-env": "7",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "browserify-shim": "^3.8.14",
    "http-server": "^0.11.1",
    "watchify": "^3.11.1"
  }
}
